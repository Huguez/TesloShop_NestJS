							"script": {
								"exec": [
									"\r",
									"pm.test(\"save token in response\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"\r",
									"    if( jsonData[\"token\"] ){\r",
									"        var tokenValue = jsonData[\"token\"];\r",
									"        pm.expect( tokenValue ).to.be.a('string'); // Check if the property is a string\r",
									"        pm.environment.set( \"token\", tokenValue );\r",
									"    } else {\r",
									"        pm.expect.fail(\"'token' property is missing in the response body\");\r",
									"    }\r",
